<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m" controllerName="poc.fiori.wechat.CategoryDetail" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>SEARCH_TITLE}" id="CategorySearch" showNavButton="true" navButtonPress="onCatBack">
		<content>
			<List id="idCatList" selectionChange="onCatSelectionChange" itemPress="onCatItemPress">
			</List>
		</content>
	<footer>
      <Bar>
        <contentRight>
          <Button icon="sap-icon://cart" press="onCartPress" />
        </contentRight>
      </Bar>
    </footer>
	</Page>
</core:View>